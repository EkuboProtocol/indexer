{
  "name": "@ekubo/indexer",
  "private": true,
  "version": "0.0.1",
  "description": "Indexes Ekubo data into a Postgres database",
  "scripts": {
    "rhc:sepolia": "NETWORK_TYPE=evm NETWORK=rhc-sepolia bun src/index.ts",
    "base:sepolia": "NETWORK_TYPE=evm NETWORK=base-sepolia bun src/index.ts",
    "base:mainnet": "NETWORK_TYPE=evm NETWORK=base-mainnet bun src/index.ts",
    "arb:sepolia": "NETWORK_TYPE=evm NETWORK=arb-sepolia bun src/index.ts",
    "arb:mainnet": "NETWORK_TYPE=evm NETWORK=arb-mainnet bun src/index.ts",
    "polygon:mainnet": "NETWORK_TYPE=evm NETWORK=polygon-mainnet bun src/index.ts",
    "bsc:mainnet": "NETWORK_TYPE=evm NETWORK=bsc-mainnet bun src/index.ts",
    "eth:mainnet": "NETWORK_TYPE=evm NETWORK=mainnet bun src/index.ts",
    "eth:sepolia": "NETWORK_TYPE=evm NETWORK=sepolia bun src/index.ts",
    "starknet:mainnet": "NETWORK_TYPE=starknet NETWORK=mainnet bun src/index.ts",
    "starknet:sepolia": "NETWORK_TYPE=starknet NETWORK=sepolia bun src/index.ts",
    "sync-tokens": "bun scripts/sync-tokens.ts",
    "sync-token-prices": "bun scripts/sync-token-prices.ts",
    "migrate": "bun scripts/migrate.ts",
    "completeness-test": "bun scripts/completeness-test.ts"
  },
  "author": "Moody Salem",
  "type": "module",
  "engines": {
    "bun": ">=1.3"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.15",
    "@types/bun": "^1.3.9"
  },
  "dependencies": {
    "@apibara/evm": "^2.1.0-beta.54",
    "@apibara/evm-rpc": "^2.1.0-beta.55",
    "@apibara/protocol": "^2.1.0-beta.55",
    "@apibara/starknet": "^2.1.0-beta.55",
    "dotenv": "^17.3.1",
    "postgres": "^3.4.8",
    "postgres-shift": "^0.1.0",
    "viem": "^2.46.0",
    "winston": "^3.19.0"
  }
}
